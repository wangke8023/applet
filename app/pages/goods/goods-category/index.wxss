.VerticalNav.nav {
  width: 220rpx;
  white-space: initial;
}
.VerticalNav.nav .cu-item {
  font-size:14px;
  font-weight:300;
  color: #333333;
  width: 100%;
  text-align: center;
  background-color: #f1f1f1;
  margin: 0;
  border: none;
  height: 50px;
  position: relative;
}
.VerticalNav.nav .cu-item.cur {
  font-size:15px;
  font-weight:500;
  border-radius: 10rpx 0 0 10rpx;
  background-color: #fff;
}
.VerticalNav.nav .cu-item.cur::after {
  content: "";
  position: absolute;
  background-color: currentColor;
  top: 0;
  right: 0rpx;
  bottom: 0;
  margin: auto;
}
